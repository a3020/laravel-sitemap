{
    "name": "a3020/sitemap",
    "description": "Sitemap validator",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "authors": [
        {
            "name": "a3020",
            "email": "a3020@fastmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "A3020\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "A3020\\Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "A3020\\Sitemap\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5@dev"
    }
}
